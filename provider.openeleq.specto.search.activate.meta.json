{
 "name": "Specto Search Direct Play (Q)",
 "plugin": "plugin.video.specto",
 "id": "provider.openeleq.specto.direct.play",
 "movies": [
  [
   {
    "link": "plugin://plugin.video.specto/?action=movieSearch&query={title}",
    "steps": ["{title}.*({year}).*"],
    "action": "PLAY"
   }
  ]
 ],
 "tvshows": [
  [
   {
    "link": "plugin://plugin.video.specto/?action=tvSearch&query={clearname}",
    "steps": ["{clearname}","Season {season}","{season}x{episode:02d}.*{title}"],
    "action": "PLAY"
   }
  ]
 ]
}
